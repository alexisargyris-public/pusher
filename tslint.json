{
  "extends": ["plugin:prettier/recommended"],
  "rules": {
    "no-string-throw": true,
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "no-conditional-assignment": true,
    "curly": [true, "as-needed"],
    "class-name": true,
    "triple-equals": true
  },
  "defaultSeverity": "warning"
}
