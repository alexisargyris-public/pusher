{
  "name": "pusher",
  "displayName": "pusher",
  "description": "vscode extension to push update events of the active document to a remote server",
  "version": "1.4.0",
  "publisher": "anemomazomata",
  "repository": {
    "type": "git",
    "url": "https://github.com/anemomazomata/pusher.git"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.pusher"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.pusherStart",
        "title": "Pusher: start"
      },
      {
        "command": "extension.pusherStop",
        "title": "Pusher: stop"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.10.2",
    "ajv": "^6.7.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.7.0",
    "prettier": "^1.16.3",
    "tslint": "^5.12.1",
    "typescript": "^2.6.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link": "^1.2.8",
    "apollo-link-http": "^1.5.11",
    "aws-appsync": "1.3.3",
    "aws-sdk": "^2.395.0",
    "es6-promise": "^4.1.1",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.10.1",
    "isomorphic-fetch": "^2.2.1",
    "lz-string": "^1.4.4",
    "promise-waterfall": "^0.1.0",
    "ws": "^3.3.1"
  },
  "license": "MIT"
}
